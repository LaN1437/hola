<!--start overlay-->
<div class="overlay toggle-menu"></div>
<!--end overlay-->
<!--Start sidebar-wrapper-->
<div id="sidebar-wrapper" data-simplebar="" data-simplebar-auto-hide="true">
    <div class="brand-logo">
     <a>
      <img src="<%= settings.logo.url %>" class="logo-icon rotate" alt="logo icon">
      <h5 class="logo-text"><%= settings.name %></h5>
    </a>
  </div>
  <ul class="sidebar-menu do-nicescrol">
     <li>
       <a href="../dashboard">
         <i class="zmdi zmdi-view-dashboard"></i> <span>Dashboard</span>
       </a>
     </li>

     <li>
      <a href="../servers">
        <i class="fa-solid fa-server"></i> <span>Servers</span>
      </a>
    </li>

     <li>
       <a href="../new">
        <i class="fa-solid fa-plus"></i> <span>Create</span>
       </a>
     </li>

     <% if (settings.coins.store.enabled == true) { %>
     <li>
       <a href="../earn">
        <i class="fa-solid fa-coins"></i> <span>Earn</span>
       </a>
     </li>

     <% if (settings.earn.referral.enabled == true) { %>
     <li>
      <a href="../referral">
       <i class="bi bi-person-fill-add"></i> <span>Refer</span>
      </a>
    </li>
    <% } %>

      <% if (settings.earn.arcio.enabled == true) { %>
        <% if (settings.earn.arcio["afk page"].enabled == true) { %>
     <li>
      <a href="../afk">
        <i class="fa-solid fa-clock"></i> <span>AFK</span>
      </a>
    </li>
    <% } %>
    <% } %>
     <li>
       <a href="../marketplace">
        <i class="fa-solid fa-store"></i> <span>Marketplace</span>
       </a>
     </li>

     <li>
       <a href="../exchange">
        <i class="fa-solid fa-gift"></i> <span>Exchange</span>
       </a>
     </li>
     <% } %>

     <li>
      <a href="../settings">
        <i class="fa-solid fa-user"></i> <span>Account</span>
      </a>
    </li>

     <li>
      <a href="../links">
        <i class="fa-solid fa-link"></i> <span>Links</span>
      </a>
    </li>

    <% if (pterodactyl.root_admin == true) { %>
      <li>
        <a href="../admin/home">
         <i class="fa-solid fa-screwdriver-wrench"></i> <span>Admin</span>
        </a>
      </li>
      <% } %>
   </ul>
  
  </div>
  <!--End sidebar-wrapper-->
  <% if (settings.logo.rotate == true) { %>
    <style>
      .rotate {
animation: rotation <%= settings.logo.speed %> infinite linear;
}

@keyframes rotation {
from {
transform: rotate(0deg);
}
to {
transform: rotate(360deg);
}
}
      </style>
      <% } %>
      <!-- sidebar-menu js -->
      <script src="/assets/js/sidebar-menu.js"></script>