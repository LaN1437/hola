<script>
    function onboarding() {
      fetch('/onboarding/status')
          .then(response => response.json())
          .then(data => {
              if (data.status == false) {
                  window.location.href = "/onboarding"
              }
          })
          .catch(error => {
              console.error('Error:', error);
          });
  }

  onboarding();

  function birthday() {
        fetch('/birthday/status')
            .then(response => response.json())
            .then(data => {
                if (data.status == true && data.wished == false) {
                    window.location.href = "/birthday"
                }
            })
            .catch(error => {
                console.error('Error:', error);
            });
    }

    birthday();
</script>
